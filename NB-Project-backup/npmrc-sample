 echo "email=syedbadruddoja@my.unt.edu" > d.prenpmrc
 echo "always-auth=true" > e.prenpmrc
 echo "strict-ssl=false" > f.prenpmrc
 cat *.prenpmrc > .npmrc
 rm *.prenpmrc



